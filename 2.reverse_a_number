#include<iostream>
#include<string>
using namespace std;



int main(){
//reverse a number;
	int n;
	cin>>n;
	string s;



	s = to_string(n);    //usign to_string() function;
	n = s.length();
	for(int i=0;i<n;i++){
		cout<<s[n-i-1];
	}
  
  
  
//direct through string

string s;
cin>>s;
int  n = s.length();
for(int i{0};i<n;i++){
    cout<<s[n-i-1]<<endl;
}



return 0;
}
