#include<iostream>
#include<math.h>
using namespace std;





int main(){
int n{};
cin>>n;
int sum{},last_digit{};
while(n>0){
        last_digit = n%10;
        sum += pow(last_digit,3);
        n = (n/10);
}
cout<<sum<<endl;
if(sum==n){
    cout<<"Tes!,its an armstrong!"<<endl;
}else{
    cout<<"nopes not an armstrong!"
}


    return 0;
}
